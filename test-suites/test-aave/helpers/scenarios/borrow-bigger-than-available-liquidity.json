          {
            "name": "deposit",
            "args": {
              "reserve": "DAI",
              "amount": "1000",
              "user": "0"
            },
            "expected": "success"
          },
          {
            "name": "mint",
            "args": {
              "reserve": "WETH",
              "amount": "1",
              "user": "1"
